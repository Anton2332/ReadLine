{
  "name": "@read-line/backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "check:env": "node ./check-env.js",
    "prebuild": "rimraf dist",
    "build": "yarn generate && yarn check:env && nest build",
    "format": "prettier --write src/**/*.ts",
    "start": "yarn generate && yarn check:env && nest start",
    "dev": "yarn generate && NODE_ENV=dev yarn check:env && NODE_ENV=dev nest start --watch",
    "debug": "NODE_ENV=dev nest start --debug --watch",
    "prod": "yarn generate && node dist/main",
    "generate": "prisma generate",
    "migrate:dev": "prisma generate && dotenv -e .env.dev -- prisma migrate dev",
    "db:push:prod": "dotenv -e .env.prod -- yarn prisma db push",
    "db:push:stage": "dotenv -e .env.staging -- yarn prisma db push",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@algolia/client-search": "^4.19.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^2.0.1",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "^4.10.1",
    "@types/algoliasearch": "^4.0.0",
    "algoliasearch": "^4.19.1",
    "aws-sdk": "^2.1331.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^7.2.1",
    "express-session": "^1.17.3",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.18",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "prisma": "^4.10.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0"
  },
  "prisma": {
    "schema": "./src/db/schema.prisma",
    "seed": "ts-node ./src/db/seed.ts"
  }
}
